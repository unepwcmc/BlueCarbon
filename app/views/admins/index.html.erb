<div class="page-header">
  <%= link_to new_admin_path, :class => 'btn btn-primary' do %>
    <i class="icon-plus icon-white"></i>
    New User
  <% end %>
  <h1>Listing users</h1>
</div>

<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>Email</th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
<% @admins.each do |admin| %>
    <tr>
      <td><%= admin.email %></td>
      <td><%= link_to 'Show', admin %></td>
      <td><%= link_to 'Edit', edit_admin_path(admin) %></td>
      <td><%= link_to 'Delete', admin_path(admin), :method => :delete, :confirm => "Are you sure?" %></td>
    </tr>
<% end %>
  </tbody>
</table>
